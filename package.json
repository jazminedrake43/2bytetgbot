{
  "name": "2bytetgbot",
  "version": "1.0.0",
  "description": "A TypeScript library and CLI for creating Telegram bots with sections-based architecture",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "2byte": "./bin/2byte-cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "prepare": "npm run build",
    "pack": "npm run build && npm pack",
    "publish:local": "npm run build && npm pack && npm install -g 2bytetgbot-1.0.0.tgz"
  },
  "keywords": [
    "telegram",
    "bot",
    "typescript",
    "sections",
    "cli",
    "generator",
    "framework"
  ],
  "author": "2byte",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.0.0",
    "mustache": "^4.2.0",
    "inquirer": "^9.2.0",
    "chalk": "^5.3.0",
    "fs-extra": "^11.1.1",
    "dotenv": "^16.6.1",
    "telegraf": "^4.16.3",
    "telegram": "^2.26.22",
    "socks": "^2.8.6"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/mustache": "^4.2.6",
    "@types/node": "^20.19.8",
    "@types/inquirer": "^9.0.3",
    "@types/fs-extra": "^11.0.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "bun-types": "^1.2.18",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3"
  },
  "files": [
    "dist/**/*",
    "bin/**/*",
    "templates/**/*",
    "README.md",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=16.0.0"
  }
}